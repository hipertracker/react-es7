{
  "name": "react-es7",
  "version": "0.0.17",
  "description": "Example of using React + Reflux + router + ES6 with experimental ES7 syntax + Jest + Semantic-UI",
  "main": "src/index.js",
  "scripts": {
    "test": "BABEL_JEST_STAGE=0 jest",
    "bundle": "broccoli build dist",
    "serve": "broccoli serve"
  },
