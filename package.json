  "license": "MIT",
  "devDependencies": {
    "jest-cli": "^0.4.0",
    "babel-jest": "^5.0.1",
    "broccoli": "^0.15.3",
    "broccoli-babel-transpiler": "^5.0.0",
    "broccoli-env": "0.0.1",
    "broccoli-fast-browserify": "^0.2.5",
    "broccoli-funnel": "^0.2.3",
    "broccoli-gzip": "^0.2.1",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-sass": "^0.6.3",
    "broccoli-timepiece": "^0.3.0",
    "broccoli-uglify-js": "^0.1.3",
    "rimraf": "^2.3.2"
  },
  "dependencies": {
    "lodash": "^3.6.0",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
